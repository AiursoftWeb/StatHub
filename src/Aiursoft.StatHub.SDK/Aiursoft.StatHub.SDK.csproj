<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <Version>1.0.8</Version>
    <TargetFramework>net7.0</TargetFramework>
    <AssemblyName>Aiursoft.StatHub.SDK</AssemblyName>
    <RootNamespace>Aiursoft.StatHub.SDK</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Company>Aiursoft</Company>
    <Product>SDK</Product>
    <Description>Nuget package of 'SDK' provided by Aiursoft</Description>
    <PackageId>Aiursoft.StatHub.SDK</PackageId>
    <PackageTags>nuget package dotnet csproj dependencies</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\README.md" Pack="true" PackagePath="." />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Aiursoft.AiurObserver" Version="7.0.12" />
    <PackageReference Include="Aiursoft.AiurProtocol" Version="7.0.17" />
  </ItemGroup>
</Project>
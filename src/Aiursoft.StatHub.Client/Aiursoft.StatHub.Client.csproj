<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <Version>8.0.24</Version>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>stathub-client</AssemblyName>
    <RootNamespace>Aiursoft.StatHub.Client</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>stathub-client</ToolCommandName>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Company>Aiursoft</Company>
    <Product>StatHub.Client</Product>
    <Description>Nuget package of 'StatHub.Client' provided by Aiursoft</Description>
    <PackageId>Aiursoft.StatHub.Client</PackageId>
    <PackageTags>nuget package stat dotnet cli tool</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://gitlab.aiursoft.cn/aiursoft/stathub</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://gitlab.aiursoft.cn/aiursoft/stathub.git</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Aiursoft.AiurObserver.Command" Version="8.0.9" />
    <PackageReference Include="Aiursoft.CommandFramework" Version="8.0.6" />
    <None Include="..\..\README.md" Pack="true" PackagePath="." />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Aiursoft.StatHub.SDK\Aiursoft.StatHub.SDK.csproj" />
  </ItemGroup>
</Project>